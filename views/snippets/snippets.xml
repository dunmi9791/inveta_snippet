<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <template id="inveta_snippets" inherit_id="website.snippets" name="Inveta Snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div id="snippet_structure" class="o_panel">
                <div class="o_panel_header">Inveta</div>
                <div class="o_panel_body">

                     <t t-snippet="inveta_snippet.dynamic_improved_ads" t-thumbnail="/inveta_snippet/static/src/img/snippets/dynamic.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.multi_video" t-thumbnail="/inveta_snippet/static/src/img/snippets/youtube.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.discover_inveta" t-thumbnail="/inveta_snippet/static/src/img/snippets/discover.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.inveta_mission" t-thumbnail="/inveta_snippet/static/src/img/snippets/mission.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.our_services" t-thumbnail="/inveta_snippet/static/src/img/snippets/services.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.inveta_mission_collabs" t-thumbnail="/inveta_snippet/static/src/img/snippets/mission_collabs.svg">
                        <keywords>ads, section</keywords>
                    </t>
                     <t t-snippet="inveta_snippet.s_our_history_content" t-thumbnail="/inveta_snippet/static/src/img/snippets/history.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.s_our_home_content" t-thumbnail="/inveta_snippet/static/src/img/snippets/home.svg">
                        <keywords>ads, section</keywords>
                    </t>
                    <t t-snippet="inveta_snippet.our_journey" t-thumbnail="/inveta_snippet/static/src/img/snippets/journey.svg">
                        <keywords>ads, section</keywords>
                    </t>

                </div>
            </div>
        </xpath>
    </template>



</odoo>